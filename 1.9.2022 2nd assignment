#include<stdio.h>
void factorial_without_return(int n)
{
  int fact=1;
	while(n>0)
	{
		fact*=n;
		n--;
	}
	printf("factorial is %d",n,fact);
}
int factorial_with_return(int n)
{
	int fact=1;
	while(n>0)
	{
		fact*=n;
		n--;
	}
	return fact;
}

void main()
{
	int n,result;
	scanf("%d",&n);
	printf("without return value:");
	factorial_without_return(n);
	printf("\nwith return value:");
	result=factorial_with_return(n);
	printf("factorial is %d",result);
}
